{
	"name": "protect-ukraine-now",
	"description": "Protect Ukraine Now",
	"version": "4.0.1",
	"type": "module",
	"private": true,
	"license": "MIT",
	"homepage": "https://protectukrainenow.org",
	"scripts": {
		"dev": "vite",
		"dev:pun": "cross-env VITE_APP_NAME='pun' vite",
		"dev:uat": "cross-env VITE_APP_NAME='uat' vite",
		"load": "pnpm load:sheets && pnpm load:vega",
		"load:sheets": "node src/data/load-sheets.js",
		"load:vega": "node src/data/load-vega.js",
		"build": "pnpm load && rakkas build",
		"preview": "cross-env NODE_ENV='development' rakkas build && wrangler dev",
		"switch:stage": "sed -i 's/prod/stage/g' wrangler.toml",
		"switch:prod": "sed -i 's/stage/prod/g' wrangler.toml",
		"deploy:stage": "pnpm build && wrangler deploy",
		"deploy:prod": "pnpm build && pnpm switch:prod && wrangler deploy"
	},
	"dependencies": {
		"@hattip/adapter-cloudflare-workers": "^0.0.35",
		"@iconify-json/circle-flags": "^1.1.45",
		"@iconify-json/healthicons": "^1.1.12",
		"@iconify-json/ion": "^1.1.14",
		"@iconify-json/map": "^1.1.8",
		"@iconify-json/material-symbols": "^1.1.68",
		"@iconify-json/mdi": "^1.1.58",
		"@unocss/preset-icons": "^0.58.0",
		"clsx": "^2.0.0",
		"cross-env": "^7.0.3",
		"daisyui": "3",
		"date-fns": "^2.30.0",
		"markdown-to-jsx": "^7.3.2",
		"rakkasjs": "^0.6.20",
		"rambda": "^8.6.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"reset-css": "^5.0.2",
		"sass": "^1.69.5",
		"unocss": "^0.58.0",
		"unocss-preset-daisy": "^7.0.0",
		"vega": "^5.26.1",
		"vega-lite": "^5.16.3",
		"vite": "4"
	},
	"devDependencies": {
		"@rakkasjs/eslint-config": "0.6.20",
		"@types/react": "^18.2.41",
		"@types/react-dom": "^18.2.17",
		"dayjs": "^1.11.10",
		"eslint": "^8.55.0",
		"typescript": "^5.3.2",
		"wrangler": "^3.18.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/protect-ukraine-now/preact-netlify.git"
	},
	"packageManager": "pnpm@9.0.6+sha256.5858806c3b292cbec89b5533662168a957358e2bbd86431516d441dc1aface89"
}
